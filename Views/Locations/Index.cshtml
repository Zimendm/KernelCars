@model List<Location>

<table class="table table-hover table-sm">
    <thead class="thead-light">
        <tr>
            <th>Id</th>
            <th>Размещение</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ID</td>
                <td>@item.LocationName</td>
            </tr>
            
        }
    </tbody>
</table>

